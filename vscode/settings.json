{
    "editor.fontSize": 17,
    "editor.fontFamily": "'Hack', 'Chalkboard SE', 'Cascadia Code NF SemiLight', 'UbuntuMonoDerivativePowerline Nerd Font', 'Consolas', 'Hack', 'Consolas NF', Menlo, Monaco, 'Courier New', monospace",
    "editor.fontWeight": "normal",
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "editor.lineHeight": 1.6,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "k"
            ],
            "after": [
                "<Esc>"
            ]
        },
        {
            "before": [
                "<C-e>"
            ],
            "commands": [
                "cursorLineEnd"
            ]
        },
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "d"
            ],
            "after": [
                "d",
                "d"
            ]
        },
        {
            "before": [
                "<C-n>"
            ],
            "commands": [
                ":nohl"
            ]
        }
    ],
    "vim.leader": "<space>",
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false
    },
    "workbench.editor.enablePreview": false,
    "debug.console.fontSize": 16,
    "debug.toolBarLocation": "docked",
    "editor.minimap.enabled": false,
    "files.exclude": {
        "**/.idea": true,
        "**/dart": true,
        "**/js": false,
        "**/node_modules": true,
        "**/py": true,
    },
    "workbench.iconTheme": "material-icon-theme",
    "editor.wordWrapColumn": 120,
    "editor.rulers": [
        120
    ],
    "workbench.colorCustomizations": {
        "editorRuler.foreground": "#505050",
        "workbench.colorCustomizations": {}
    },
    "explorer.autoReveal": false,
    "leetcode.endpoint": "leetcode-cn",
    "leetcode.workspaceFolder": "/Users/vuri/.leetcode",
    "leetcode.defaultLanguage": "golang",
    "leetcode.hint.configWebviewMarkdown": false,
    "terminal.integrated.fontFamily": "Hack Nerd Font",
    "terminal.external.osxExec": "WezTerm.app",
    "terminal.integrated.fontSize": 16,
    "workbench.editor.closeEmptyGroups": false,
    "explorer.confirmDragAndDrop": false,
    "files.autoSave": "off",
    "[html]": {
        "editor.tabSize": 2
    },
    "[css]": {
        "editor.tabSize": 2
    },
    "[javascript]": {
        "editor.tabSize": 2,
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[vue]": {
        "editor.tabSize": 2
    },
    "[typescript]": {
        "editor.tabSize": 2
    },
    "go.testFlags": [
        "-v"
    ],
    "go.toolsManagement.autoUpdate": true,
    "go.formatTool": "goimports",
    "go.toolsEnvVars": {
        "GOPROXY": "https://goproxy.io,direct"
    },
    "protoc": {
        "options": [
            "--proto_path=/Users/vuri/go/src"
        ]
    },
    "[proto3]": {
        "editor.defaultFormatter": "xaver.clang-format",
        "editor.formatOnSave": true
    },
    "workbench.sideBar.location": "left",
    "editor.renderControlCharacters": false,
    "powermode.enabled": false,
    "powermode.enableShake": false,
    "vim.cursorStylePerMode.visualblock": "block",
    "vim.cursorStylePerMode.visualline": "block",
    "editor.cursorStyle": "line",
    "window.zoomLevel": 1,
    "extensions.autoUpdate": false,
    "http.proxy": "http://localhost:1087",
    "gopls": {},
    "terminal.integrated.env.osx": {
        "FIG_NEW_SESSION": "1"
    },
    "workbench.activityBar.location": "top",
    "workbench.editor.empty.hint": "hidden",
    "workbench.startupEditor": "none",
    "editor.fontLigatures": true,
    "workbench.preferredDarkColorTheme": "Catppuccin Mocha",
    "workbench.settings.editor": "json",
    "editor.inlayHints.enabled": "offUnlessPressed",
    "workbench.editor.editorActionsLocation": "hidden",
    "rust-analyzer.check.command": "clippy",
    "rust-analyzer.check.extraArgs": [
        "--",
        "-D",
        "warnings",
        "-A",
        "unused"
    ],
    "rust-analyzer.checkOnSave": true,
    "rust-analyzer.lens.enable": false,
    "explorer.confirmDelete": false,
    "commitollama.custom.endpoint": "http://ollama.xiaoe-tools.com",
    "commitollama.custom.model": "glm4:latest",
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]",
        "todo!()"
    ],
    "mermaid-editor.generate.type": "png",
    "workbench.colorTheme": "Best Themes - Catppuccin",
    "chat.editing.alwaysSaveWithGeneratedChanges": true,
}