{
    "editor.fontSize": 18,
    "editor.fontFamily": "'Hack', 'UbuntuMonoDerivativePowerline Nerd Font', 'Consolas', 'Hack', 'Consolas NF', Menlo, Monaco, 'Courier New', monospace",
    "editor.fontWeight": "normal",
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "k"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "d"
            ],
            "after": [
                "d",
                "d"
            ]
        },
        {
            "before": [
                "<C-n>"
            ],
            "commands": [
                ":nohl"
            ]
        }
    ],
    "vim.leader": "<space>",
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false
    },
    "workbench.editor.enablePreview": false,
    "debug.console.fontSize": 14,
    "debug.toolBarLocation": "docked",
    "editor.minimap.enabled": false,
    "files.exclude": {
        "**/.idea": true,
        "**/dart": true,
        "**/js": false,
        "**/node_modules": true,
        "**/py": true,
    },
    "workbench.iconTheme": "Monokai Pro (Filter Spectrum) Icons",
    "editor.wordWrapColumn": 120,
    "editor.rulers": [
        120
    ],
    "workbench.colorCustomizations": {
        "editorRuler.foreground": "#505050",
    },
    "explorer.autoReveal": false,
    "leetcode.endpoint": "leetcode-cn",
    "leetcode.workspaceFolder": "/Users/vuri/.leetcode",
    "leetcode.defaultLanguage": "golang",
    "leetcode.hint.configWebviewMarkdown": false,
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.fontSize": 15,
    "[markdown]": {
        "editor.defaultFormatter": "AnmolSinghJaggi.vscode-markdownfmt"
    },
    "workbench.editor.closeEmptyGroups": false,
    "explorer.confirmDragAndDrop": false,
    "files.autoSave": "off",
    "[html]": {
        "editor.tabSize": 2
    },
    "[css]": {
        "editor.tabSize": 2
    },
    "[javascript]": {
        "editor.tabSize": 2,
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[vue]": {
        "editor.tabSize": 2
    },
    "[typescript]": {
        "editor.tabSize": 2
    },
    "go.testFlags": [
        "-v"
    ],
    "go.toolsManagement.autoUpdate": true,
    "go.formatTool": "goimports",
    "go.toolsEnvVars": {
        "GOPROXY": "https://goproxy.io,direct"
    },
    "protoc": {
        "options": [
            "--proto_path=/Users/vuri/go/src"
        ]
    },
    "[proto3]": {
        "editor.defaultFormatter": "xaver.clang-format",
        "editor.formatOnSave": true
    },
    "workbench.sideBar.location": "left",
    "editor.renderControlCharacters": false,
    "workbench.colorTheme": "Gruvbox Material Dark",
    "powermode.enabled": false,
    "powermode.enableShake": false,
    "vim.cursorStylePerMode.visualblock": "block",
    "vim.cursorStylePerMode.visualline": "block",
    "editor.cursorStyle": "block",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "workbench.editor.untitled.hint": "hidden",
    "window.zoomLevel": 1,
    "extensions.autoUpdate": false,
    "http.proxy": "http://localhost:8123",
    // "vscode-neovim.neovimInitVimPaths.darwin": "/Users/vuri/.config/nvim/init.lua",
    "vscode-neovim.neovimExecutablePaths.darwin": "/usr/local/bin/nvim",
    "gopls": {
        "build.experimentalWorkspaceModule": true
    },
    "terminal.integrated.env.osx": {
        "FIG_NEW_SESSION": "1"
    },
    "workbench.activityBar.visible": false
}